{
  "name": "test-express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start dist/server.js -i max --name my-server",
    "stop": "pm2 stop my-server",
    "delete": "pm2 delete my-server",
    "restart": "pm2 restart my-server",
    "list": "pm2 list",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts --files",
    "build": "tsc",
    "prod": "npm run build && node dist/server.js"
  },
